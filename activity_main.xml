        android:inputType="textPersonName"
        android:text="Video Viewer"
        tools:layout_editor_absoluteX="175dp"
        tools:layout_editor_absoluteY="61dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="72dp"
